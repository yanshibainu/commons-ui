{
  "name": "sy-commons-ui",
  "version": "1.0.5",
  "description": "sy-commons-ui",
  "author": "sy team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yanshibainu/commons-ui"
  },
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "link": "yarn link react && yarn link react-dom && yarn link react-redux",
    "unlink": "yarn unlink react && yarn unlink react-dom && yarn unlink react-redux",
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint --fix .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:unit-coverage": "cross-env CI=1 react-scripts test --env=jsdom --coverage",
    "test:watch": "react-scripts test --env=jsdom",
    "pre-deploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "publish:npm": "yarn publish"
  },
  "pre-commit": [
    "test:lint"
  ],
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@babel/helper-regex": "^7.10.5",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^23.0.0",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.4",
    "react-scripts": "3.4.1"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "add": "^2.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-perfect-scrollbar": "^1.5.8",
    "react-split-pane": "^2.0.3",
    "rsuite": "4.8.6",
    "js-cookie": "^2.2.1",
    "moment": "^2.27.0"
  }
}
